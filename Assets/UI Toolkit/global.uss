.debug {
    bottom: 10px;
    padding-top: 10px;
    padding-right: 10px;
    padding-bottom: 10px;
    padding-left: 10px;
    display: none;
}

#root {
    font-size: 28px;
    -unity-font-definition: url("project://database/Assets/TextMesh%20Pro/Examples%20&%20Extras/Fonts/Unity.ttf?fileID=12800000&guid=f4eec857a4fdf2f43be0e9f3d1a984e7&type=3#Unity");
}

.row-center {
    flex-direction: row;
    flex-grow: 1;
    justify-content: center;
}

Button {
    border-left-color: rgb(77, 77, 77);
    border-right-color: rgb(77, 77, 77);
    border-top-color: rgb(77, 77, 77);
    border-bottom-color: rgb(77, 77, 77);
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    border-bottom-left-radius: 10px;
    border-bottom-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-left-width: 1px;
    transition-duration: 0.15s;
}

Button:hover {
    scale: 1.1 1.1;
}

Button:active {
    scale: 1.05 1.05;
}

Button.rounded {
    border-top-left-radius: 24px;
    border-top-right-radius: 24px;
    border-bottom-right-radius: 24px;
    border-bottom-left-radius: 24px;
    border-bottom-width: 0;
    border-right-width: 0;
    border-left-width: 0;
    border-top-width: 0;
}

Button.lifted {
    border-bottom-width: 5px;
    border-top-width: 1px;
}

Button.lifted:active {
    border-bottom-width: 1px;
    margin-top: 6px;
}

TextField > * {
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    overflow: visible;
}

.minibutton {
    padding: 2px;
    background-image: none;
}

.collapsable {
    transition-property: max-height;
    transition-duration: 0.5s;
    max-height: 100%;
}

.collapsed .collapsable {
    max-height: 0;
}

.field-label {
    font-size: 16px;
    margin-left: 4px;
    margin-bottom: 0;
    padding-bottom: 0;
    padding-left: 0;
    position: absolute;
    transition-duration: 0.4s;
    padding-top: 0;
    padding-right: 0;
    margin-top: -2px;
}

.empty-field .field-label {
    font-size: 20px;
    margin-left: 5px;
    margin-bottom: 0;
    padding-top: 10px;
    padding-right: 10px;
    padding-bottom: 10px;
    padding-left: 10px;
    margin-top: 16px;
    color: rgb(113, 113, 113);
}

.field-error {
    color: rgb(113, 0, 0);
    font-size: 22px;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
}

.field-row {
    flex-direction: row;
    margin-top: 18px;
}

.field-input {
    flex-grow: 1;
    height: 40px;
}

.invalid .field-input {
    border-left-color: rgb(177, 0, 0);
    border-right-color: rgb(177, 0, 0);
    border-top-color: rgb(177, 0, 0);
    border-bottom-color: rgb(177, 0, 0);
    border-top-width: 1px;
    border-right-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    border-bottom-left-radius: 4px;
}

.disabled {
    display: none;
}

.hidden {
    display: none;
}

.back-button {
    align-self: flex-start;
}

.exit-button {
}

.panel {
    padding-top: 10px;
    padding-right: 10px;
    padding-bottom: 10px;
    padding-left: 10px;
    -unity-slice-scale: 1px;
    background-color: rgb(214, 214, 214);
    border-top-left-radius: 12px;
    border-top-right-radius: 12px;
    border-bottom-right-radius: 12px;
    border-bottom-left-radius: 12px;
    border-top-width: 4px;
    border-right-width: 4px;
    border-bottom-width: 4px;
    border-left-width: 4px;
    border-left-color: rgb(119, 119, 119);
    border-right-color: rgb(119, 119, 119);
    border-top-color: rgb(119, 119, 119);
    border-bottom-color: rgb(119, 119, 119);
}

.abs-center {
    position: absolute;
    top: 50%;
    left: 50%;
    translate: -50% -50%;
}

.standalone-error {
    align-self: center;
    border-left-color: rgb(41, 0, 0);
    border-right-color: rgb(41, 0, 0);
    border-top-color: rgb(41, 0, 0);
    border-bottom-color: rgb(41, 0, 0);
    border-top-width: 1px;
    border-right-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
    background-color: rgba(255, 0, 0, 0.18);
    -unity-text-align: upper-center;
    white-space: normal;
    padding-right: 10px;
    padding-left: 10px;
    margin-top: 16px;
    margin-bottom: 16px;
    margin-left: 16px;
    margin-right: 16px;
}

.confirmation-panel {
    font-size: 32px;
    padding-top: 12px;
    padding-right: 12px;
    padding-bottom: 12px;
    padding-left: 12px;
    max-width: 550px;
}

.cancel-button {
}

.hidden {
    display: none;
}

.confirm-button {
    background-color: rgb(117, 255, 154);
    margin-left: 48px;
}

.full-cover {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    -unity-background-image-tint-color: rgb(0, 0, 0);
}
